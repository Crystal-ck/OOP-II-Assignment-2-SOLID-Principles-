public interface Human {
    public void getName();
    public void sleep();
    public void eat();
}

package Interface;

public interface Work {
    public void jobTitle ();
}

import java.util.Scanner;
public class Woman implements Human, Work {
    Scanner sc = new Scanner(System.in);

    public void getName(){
        System.out.println("Enter name: ");
        String name = sc.next();
        System.out.println("Her name is: " + name);
    }
    public void sleep() {
        System.out.println("She sleeps regularly");
    }
    public void eat() {
        System.out.println("What does she eat? ");
        String food = sc.next();
        System.out.println("She eats " + food);
    }
    public void jobTitle () {
        System.out.println("What does she do for work: ");
        String job_title = sc.next();
        System.out.println("She works as a: " + job_title);
    }

    public static void main(String[] args) {
        Woman woman1 = new Woman();
        woman1.getName();;
        woman1.eat();
        woman1.sleep();
        woman1.jobTitle();
    }
}

public class Doctor extends Woman {//Breaks the rule
    public static void main (String[] args) {
        Woman doctor = new Woman();
        doctor.getName();
        doctor.eat();
        doctor.sleep();
        doctor.jobTitle();
    }

}

